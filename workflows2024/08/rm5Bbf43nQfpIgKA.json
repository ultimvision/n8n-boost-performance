        "schemaType": "manual",
        "inputSchema": "{\n  \"type\": \"object\",\n  \"properties\": {\n    \"interview\": {\n      \"type\": \"object\",\n      \"properties\": {\n        \"title\": {\n          \"type\": \"string\",\n          \"description\": \"The title of the interview.\"\n        },\n        \"participants\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"owner\": {\n              \"type\": \"object\",\n              \"properties\": {\n                \"name\": {\n                  \"type\": \"string\",\n                  \"description\": \"The name of the product owner.\"\n                },\n                \"role\": {\n                  \"type\": \"string\",\n                  \"description\": \"The role of the product owner, typically 'Product Owner'.\"\n                }\n              },\n              \"required\": [\"name\", \"role\"]\n            },\n            \"interviewer\": {\n              \"type\": \"object\",\n              \"properties\": {\n                \"name\": {\n                  \"type\": \"string\",\n                  \"description\": \"The name of the interviewer.\"\n                },\n                \"role\": {\n                  \"type\": \"string\",\n                  \"description\": \"The role of the interviewer, typically 'Interviewer'.\"\n                }\n              },\n              \"required\": [\"name\", \"role\"]\n            }\n          },\n          \"required\": [\"owner\", \"interviewer\"]\n        },\n        \"introduction\": {\n          \"type\": \"string\",\n          \"description\": \"The introduction text where the interviewer welcomes the product owner and introduces the product.\"\n        },\n        \"questions_and_answers\": {\n          \"type\": \"array\",\n          \"description\": \"An array of objects representing the questions asked by the interviewer and the corresponding answers from the product owner.\",\n          \"items\": {\n            \"type\": \"object\",\n            \"properties\": {\n              \"question\": {\n                \"type\": \"string\",\n                \"description\": \"The question asked by the interviewer.\"\n              },\n              \"answer\": {\n                \"type\": \"string\",\n                \"description\": \"The answer given by the product owner.\"\n              }\n            },\n            \"required\": [\"question\", \"answer\"]\n          }\n        },\n        \"conclusion\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"interviewer_summary\": {\n              \"type\": \"string\",\n              \"description\": \"The summary of the interview provided by the interviewer.\"\n            },\n            \"owner_final_thoughts\": {\n              \"type\": \"string\",\n              \"description\": \"The final thoughts or call to action given by the product owner.\"\n            }\n          },\n          \"required\": [\"interviewer_summary\", \"owner_final_thoughts\"]\n        }\n      },\n      \"required\": [\"title\", \"participants\", \"introduction\", \"questions_and_answers\", \"conclusion\"]\n    }\n  },\n  \"required\": [\"interview\"]\n}"
      },
